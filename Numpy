import numpy as np

A = np.array([[2, 3], [1, -1]])
B = np.array([7, 1])
x = np.linalg.solve(A, B)
print("\nNilai variabel untuk Sistem Persamaan 1:")
print("Nilai x:", x[0])
print("Nilai y:", x[1])

A2 = np.array([[1, 2, 1], [3, -1, 2], [-2, 3, -1]])
b2 = np.array([10, 5, -9])
x2 = np.linalg.solve(A2, b2)
print("\nNilai variabel untuk Sistem Persamaan 2:")
print(f"x = {x2[0]}")
print(f"y = {x2[1]}")
print(f"zÂ = {x2[2]}")
